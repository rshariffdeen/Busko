<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Busko\EntityBundle\Entity\Branches" table="branches">
    <unique-constraints>
      <unique-constraint name="branch_id" columns="branch_id"/>
    </unique-constraints>
    <id name="branchId" type="string" column="branch_id" length="10">
     
    </id>
    <field name="phone" type="string" column="phone" length="10" nullable="false"/>
    <field name="streetno" type="string" column="streetno" length="10" nullable="false"/>
    <field name="street" type="string" column="street" length="20" nullable="false"/>
    <field name="city" type="string" column="city" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
