                                            <td></td>
                                            <td align="left">{{ form_widget(search2.search)}}</td>   
                                            {{form_end(search2)}}
                                            </div>
                                    <td></td>
                                        
                                        </tr>
     
                                    </tbody>
                                </table>
                            </ul>

                        </div></div>
                </div>
            </div>
        </div>
    </div>
</div>
    



    {% endblock %}
