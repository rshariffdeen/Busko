{% extends "BuskoStyleBundle:Home:basic.html.twig" %}
{% block admin%}
{% endblock %}
{% block content %}
<div class="container-fluid">
    <div class="row-fluid">

        <noscript>
        <div class="alert alert-block span10">
            <h4 class="alert-heading">Warning!</h4>
            <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
        </div>
        </noscript>


        <div id="content" class="span10">
            <div class="row-fluid">
                <div class="box span12">
                    <div class="box-header well">
                        <h2><i class="icon-gear"></i> Bus Stops</h2>

                    </div>
                    <div class="box-content">
                        <div class="box-content">
                            <ul class="dashboard-list">
                                <div class="pull-right">
                               
                                </div>
                                <br><br>
                                <table class="records_list" width="100%">
                                    <thead>
                                        <tr>
                                            <th width="15%"></th>
                                            <th width="20%"></th> 
                                            <th width="5%"></th>  
                                            <th width="20%"></th>
                                            <th width="5%"></th>  
                                            <th width="20%"></th>
                                            <th width="15%"></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr>
                                            <div class ="form-horizontal center">
                                                {{form_start(search)}}
                                                <td></td>
                                            <td align="right">{{ form_label(search.city) }}</td>
                                            <td></td>
                                            <td align="center">{{ form_widget(search.city) }}</td>
                                            <td></td>
                                            <td align="left">{{ form_widget(search.search)}}</td>   
                                            {{form_end(search)}}
                                            </div>
                                    <td></td>
                                        
                                        </tr>
     
                                    </tbody>
                                </table>
                            </ul>

                        </div></div>
                </div>
            </div>
        
        
            <div class="row-fluid">
                <div class="box span12">
                    <div class="box-header well">
                        <h2><i class="icon-gear"></i>Routes</h2>

                    </div>
                    <div class="box-content">
