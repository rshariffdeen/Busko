                            <ul class="dashboard-list">
                                <div class="pull-right">
                                  
                                </div>
                                <br><br>
                                <table class="records_list" width="100%">
                                    <thead>
                                        <tr>
                                            <th width="15%"></th>
                                            <th width="20%"></th> 
                                            <th width="5%"></th>  
                                            <th width="20%"></th>
                                            <th width="5%"></th>  
                                            <th width="20%"></th>
                                            <th width="15%"></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr>
                                            <div class ="form-horizontal center">
                                                {{form_start(search2)}}
                                                <td></td>
                                            <td align="right">{{ form_label(search2.routeId) }}</td>
